    "@vanilla-extract/css": "^1.13.0",
    "d3-fetch": "^3.0.1",
    "d3-scale": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-simple-maps": "^3.0.0",
    "wouter": "^2.12.1"
  },
  "devDependencies": {
    "@types/d3-fetch": "^3",
    "@types/d3-scale": "^4",
    "@types/lodash": "^4",
